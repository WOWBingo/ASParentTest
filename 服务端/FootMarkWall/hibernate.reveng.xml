<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table name="user_table" catalog="footmarkdb" class="User"></table>
	<table name="message_table" catalog="footmarkdb" class="Message">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="review_table" catalog="footmarkdb" class="Review">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="wall_table" catalog="footmarkdb" class="Wall">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
	<table name="user_msg_collect_table" catalog="footmarkdb"
		class="CollectMessage">
		<primary-key>
			<generator class="increment"></generator>
		</primary-key>
	</table>
<table name="user_wall_collect_table" catalog="footmarkdb"
	class="CollectWall">
	<primary-key>
		<generator class="increment"></generator>
	</primary-key>
</table>
</hibernate-reverse-engineering>